library(data.table)
library(ggplot2)
library(reshape)
library(xtable)
data <- read.csv("r001.csv", header = F)
View(data)
dataset <- "100K"
data <- data[data$V3 == dataset,c(1,2,5)]
names(data) = c("Algorithm","Epsilon","Time")
data <- data.table(data)
data <- unique(data[, Time:=mean(Time), by=list(Algorithm, Epsilon)])
names(data) = c("Algorithm","Epsilon","Time")
data <- data.table(data)
data <- unique(data[, Time:=mean(Time), by=list(Algorithm, Epsilon)])
legend_title = "Algorithm"
#breaks = c("S0", "S1","S2","S3")
#labels = c("Baseline", "Remove evens", "Remove Bcast", "Reorder loops")
g = ggplot(data=data, aes(x=factor(Epsilon), y=Time, group=Algorithm, colour=Algorithm, shape=Algorithm)) +
geom_line(aes(linetype=Algorithm)) +
geom_point(size=2.5) +
labs(x="Epsilon (mts)", y="Time (sec)")
#pdf("plot.pdf", width = 10.5, height = 7.5)
plot(g)
